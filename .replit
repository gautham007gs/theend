[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Run Next.js App"
author = 47545497
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Next.js Dev Server"

[[workflows.workflow]]
name = "Dev Server"
author = 47876597
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx next dev --turbopack -p 3000 -H 0.0.0.0"

[[workflows.workflow]]
name = "Optimized Dev Server"
author = 48081321
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev -- --turbopack -p 5000 -H 0.0.0.0"

[[workflows.workflow]]
name = "Next.js Dev Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[[workflows.workflow]]
name = "Clean Dev Restart"
author = 48105997
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx next dev --turbopack -p 3001 -H 0.0.0.0"

[[workflows.workflow]]
name = "Fresh Dev Server"
author = 48105997
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx next dev --turbopack -p 3000 -H 0.0.0.0"

[[workflows.workflow]]
name = "Clean Production Test"
author = 48262361
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run build"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm start"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "start", "--", "-p", "5000", "-H", "0.0.0.0"]
